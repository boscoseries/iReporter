{
  "name": "i-reporter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run migrate && nyc mocha -r @babel/register --exit --timeout 20000",
    "start": "npm run create && nodemon -r @babel/register server/index.js",
    "create": "node server/models/userModel createUserTable && node server/models/incidentModel createIncidentTable",
    "drop": "node server/models/incidentModel dropIncidentTable && node server/models/userModel dropUserTable",
    "migrate": "npm run drop && npm run create",
    "coverage": "nyc reporter=lcov --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boscoseries/iReporter.git"
  },
