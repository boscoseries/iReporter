    "url": "https://github.com/boscoseries/iReporter/issues"
  },
  "homepage": "https://github.com/boscoseries/iReporter#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "ghooks": "^2.0.4",
    "joi": "^14.3.0",
    "jsonwebtoken": "^8.4.0",
    "pg": "^7.7.1",
    "uuid": "^3.3.2"
